<Window
	x:Class="Modding_assistant.windows.ProgressBar" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="clr-namespace:Modding_assistant.windows"
	Title="Progress"
	Height="30"
	Width="280"
	ResizeMode="NoResize"
	ShowInTaskbar="False"
	WindowStartupLocation="Manual"
	WindowStyle="None">
	<Grid
		Background="#FFE5E5E5">
		<Rectangle
			Fill="#FFF4F4F5"
			Stroke="#FF000000"
			Margin="0,0,0,0"
			Panel.ZIndex="1" />
		<ProgressBar
			Name="bar"
			Margin="5,5,5,5"
			Panel.ZIndex="3" />
	</Grid>
</Window>
